<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcome-file>home.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-class>controller.CreateTeams</servlet-class>
    <servlet-name>createteams</servlet-name>
  </servlet>
  <servlet-mapping>
    <servlet-name>createteams</servlet-name>
    <url-pattern>/createteams</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-class>controller.CreatePlayer</servlet-class>
    <servlet-name>createplayers</servlet-name>
  </servlet>
  <servlet-mapping>
    <servlet-name>createplayers</servlet-name>
    <url-pattern>/createplayers</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-class>controller.Home</servlet-class>
    <servlet-name>home</servlet-name>
  </servlet>
  <servlet-mapping>
    <servlet-name>home</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
</web-app>
